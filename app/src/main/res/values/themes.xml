        <!--        <item name="android:gravity">center</item>-->
        <!--        <item name="android:inputType">number</item>-->
        <item name="android:background">@drawable/edittext_shape</item>
        <item name="elevation">@dimen/_3sdp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">right|center</item>
        <item name="android:paddingStart">@dimen/_10sdp</item>
        <item name="android:paddingEnd">@dimen/_10sdp</item>
        <item name="android:textDirection">rtl</item>

        <item name="android:layout_marginTop">@dimen/_5sdp</item>
        <item name="android:forceDarkAllowed" tools:targetApi="q">false</item>

    </style>

