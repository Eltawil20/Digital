<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <!-- Base application theme. -->
    <style name="Theme.Digital" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <item name="android:forceDarkAllowed" tools:targetApi="q">false</item>

        <!-- Customize your theme here. -->
    </style>

    <style name="textView_main" >
        <item name="android:textSize">@dimen/_18ssp</item>
        <item name="fontFamily">@font/tajawal_bold</item>
        <item name="android:textColor">@color/purple_200</item>
        <item name="android:forceDarkAllowed" tools:targetApi="q">false</item>

    </style>

    <style name="textView_secondary" >
        <item name="android:textSize">@dimen/_11ssp</item>
        <item name="fontFamily">@font/tajawal_bold</item>
        <item name="android:textColor">@color/grey</item>
        <item name="android:forceDarkAllowed" tools:targetApi="q">false</item>

    </style>


    <style name="style_screen" >
        <item name="android:paddingStart">@dimen/_20sdp</item>
        <item name="android:paddingEnd">@dimen/_20sdp</item>
        <item name="android:forceDarkAllowed" tools:targetApi="q">false</item>


    </style>


